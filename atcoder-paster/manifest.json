{
  "manifest_version": 3,
  "name": "AtCoder Java Paster",
  "version": "0.2.0",
  "description": "Pick a .java file and paste into AtCoder submit editor (submit page only).",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": ["scripting"],
  "host_permissions": [
    "https://atcoder.jp/contests/abc*/tasks/abc*",
    "https://atcoder.jp/contests/abc*/submit"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+V",
        "mac": "Command+Shift+V"
      },
      "description": "Open the popup"
    }
  }
}
